{
  "manifest_version": 3,
  "name": "LeetCode Profile Tracker",
  "version": "1.0",
  "description": "Track LeetCode user profile stats",
  "permissions": ["storage"],
  "host_permissions": ["https://leetcode.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "LeetCode Tracker"
  }
}